<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="app.tracker.fittracker.ui.home.WorkoutsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_workouts" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="app.tracker.fittracker.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="app.tracker.fittracker.ui.notifications.PlansFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_plans" />
    <fragment
        android:id="@+id/planGeneration"
        android:name="app.tracker.fittracker.ui.plans.PlanGeneration"
        android:label="fragment_plan_generation"
        tools:layout="@layout/activity_plan_creator" />
</navigation>